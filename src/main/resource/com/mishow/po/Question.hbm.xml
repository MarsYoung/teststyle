<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.mishow.po.Question"
		table="t_question">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="questiontype" />
		<property name="questionanswer" />
		<property name="questioncontent" />
		<property name="questioncommitter" />
		<property name="questioncommitterid" />
		<property name="questionfrom" />
		<property name="questiontypeString"></property>
		<property name="questionsubject"></property>
		<property name="questiondifficulty"></property>
		<property name="datetime" column="createtime"></property>
		<property name="major"></property>
		<property name="isbasicsubject"></property>

	</class>
</hibernate-mapping>
